import matplotlib.pyplot as plt

f = open("a.txt", "r")
lines = f.readlines()

x = []
y = []

j = 0
for i in lines:
	if(j % 5 == 1):
		k = i.split()
		free.append(int(k[1]))
	
	if(j % 5 == 2):
		k = i.split()
		active.append(int(k[1]))

	if(j % 5 == 3):
		k = i.split()
		used.append(float(k[2][:-1]))
		shared.append(float(k[4][:-1]))
		buff.append(float(k[5][:-1]))
		cache.append(float(k[6][:-1]))
	j += 1
	

plt.title('Your title')
plt.plot(x, y, 'ro')
plt.axis([min(x), max(x), min(y), max(y)])
plt.show()

